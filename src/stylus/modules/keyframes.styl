@keyframes burguer
	0%
		opacity 0
		fill transparent
		stroke #fff
		stroke-dasharray  1000
		stroke-dashoffset  1000
		stroke-width 0.2rem
		
	19.9%
		opacity 1
		
	20%
		fill transparent
		stroke #fff
		stroke-dasharray  1000
		stroke-dashoffset  1000
		stroke-width 0.2rem

	88%
		fill transparent
		stroke-dasharray  1000
		stroke-dashoffset 0
		stroke #fff
		stroke-width 0.2rem
			
	100%
		stroke-dashoffset 0
		stroke-width 0
		stroke transparent
		fill #fff
		opacity 1